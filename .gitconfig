[user]
	name = Kerstin Puschke
	email = kerstin.puschke@shopify.com
[core]
	editor = emacs
	commitGraph = true
[color]
	ui = auto
[alias]
	co = checkout
[push]
	default = simple
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[diff]
	algorithm = patience
[protocol]
	version = 2
[gc]
	writeCommitGraph = true
[pull]
	rebase = true
